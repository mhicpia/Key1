<h1>Listing keys</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Number</th>
      <th>Facility</th>
      <th>Company</th>
      <th>Checkout</th>
      <th>Checkin</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @keys.each do |key| %>
      <tr>
        <td><%= key.description %></td>
        <td><%= key.number %></td>
        <td><%= key.facility %></td>
        <td><%= key.company %></td>
        <td><%= key.checkout %></td>
        <td><%= key.checkin %></td>
        <td><%= link_to 'Show', key %></td>
        <td><%= link_to 'Edit', edit_key_path(key) %></td>
        <td><%= link_to 'Destroy', key, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Key', new_key_path %>
